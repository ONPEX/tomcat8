s/org.apache.tomcat/javax.servlet/ servlet-api jar s/.*/3.0/
s/org.apache.tomcat/javax.servlet.jsp/ jsp-api jar s/.*/2.2/
s/org.apache.tomcat/javax.el/ el-api jar s/.*/2.2/
